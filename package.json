{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for creating webpack project",
  "main": "index.js",
  "scripts": {
    "build:dev": "npm run clean:dist && webpack --progress",
    "build": "npm run build:dev",
    "buildvendor": "webpack --progress --config webpack.config.vendor.js",
    "clean:dist": "npm run rimraf -- dist",
    "clean": "npm run clean:dist",
    "rimraf": "rimraf",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server": "npm run server:dev"
  },
  "repository": {},
  "author": "mangeg",
  "license": "ISC",
  "devDependencies": {
    "@angularclass/hmr": "^1.2.2",
    "@angularclass/hmr-loader": "^3.0.2",
    "@types/node": "^7.0.4",
    "@types/webpack": "^2.2.4",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "angular2-template-loader": "^0.6.0",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "codelyzer": "^2.0.0-beta.4",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.4.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "script-ext-html-webpack-plugin": "^1.5.0",
    "style-loader": "^0.13.1",
    "tslint": "^4.4.2",
    "typescript": "^2.1.5",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.0.0",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "^2.6.0"
  },
  "dependencies": {
    "@angular/common": "^2.4.5",
    "@angular/compiler": "^2.4.5",
    "@angular/core": "^2.4.5",
    "@angular/platform-browser": "^2.4.5",
    "@angular/platform-browser-dynamic": "^2.4.5",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.3",
    "zone.js": "^0.7.6"
  }
}
